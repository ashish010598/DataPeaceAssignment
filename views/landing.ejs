<% include partials/header %>
<div class="container"  style="padding-top: 30px;">
			<table id="usertable" class="table table-bordered">
			 <thead>
			   <tr style="background-color: gray;">
				 <th>First Name</th>
				 <th>Last Name</th>
				 <th>Company</th>
				 <th>City</th>
				 <th>State</th>
				 <th>ZIP</th>
				 <th>Email</th>
				 <th>Website</th>
				 <th>Age</th>
			   </tr>
			 </thead>
			 <tbody>
			 <% users.forEach(function(user){ %>
				<tr>
				 <td><a href="user/<%= user.id %>"><%= user.fname %></a></td> 
				 <td><a href="user/<%= user.id %>"><%= user.lname %></a></td>
				 <td><a href="user/<%= user.id %>"><%= user.company %></a></td>
				 <td><a href="user/<%= user.id %>"><%= user.city %></a></td>
				 <td><a href="user/<%= user.id %>"><%= user.state %></a></td>
				 <td><a href="user/<%= user.id %>"><%= user.zip %></a></td>
				 <td><a href="user/<%= user.id %>"><%= user.email %></a></td>
				 <td><%= user.website %></td>
				 <td><a href="user/<%= user.id %>"></a><%= user.age %></td>
			   </tr>
			 <% }); %> 
				</tbody>
		   </table>
		 </div>
<script>
	$(document).ready(function() {
    $('#usertable').DataTable( {
        "lengthMenu": [5]
    } );
} );
</script>
<% include partials/footer %>